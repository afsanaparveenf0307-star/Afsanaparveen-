<script>
/* animate language bars */
window.addEventListener('load',()=>{
  document.querySelectorAll('.bar > span').forEach(el=>{
    const lvl = Math.max(0, Math.min(100, parseInt(el.dataset.level||'0',10)));
    requestAnimationFrame(()=>{ el.style.width = lvl + '%'; });
  });
});
optional: press P to print */
document.addEventListener('keydown',e=>{
  if((e.key==='p'||e.key==='P') && (e.ctrlKey || e.metaKey)){ window.print(); }
});
</script>
